<view>

  <view>
    <!-- 头图 -->
    <image class="head-image" bindtap='previewImg' data-scr="https://ss1.bdstatic.com/70cFvXSh_Q1YnxGkpoWK1HF6hhy/it/u=3592456267,1694761420&fm=26&gp=0.jpg" mode="aspectFill" src="https://ss1.bdstatic.com/70cFvXSh_Q1YnxGkpoWK1HF6hhy/it/u=3592456267,1694761420&fm=26&gp=0.jpg"></image>


    <view class='head-title'>
      <text class='main-title'>5G未来城-超速美力</text>
    </view> 

    <block wx:for="{{posts}}" wx:for-item="item" class="left" wx:for-index="idx" wx:key="">

      <!-- <view class='artist'>
        <image src="{{item.headerUrl}}"></image>
        <view class="text-box">
          <view class="arow arow01">
            <view class="artist-name">{{item.name}}</view>
          </view>
          <view class="arow">
            <view class="time">{{item.timestamp}}</view>
          </view>
        </view>
      </view>

      <!-- 文字 -->
      <!-- <view class='post'>
        <text class='main-post'>{{item.content}}</text>
      </view> -->

      <!-- 图片展示 -->
      <view class='pic'>

        <!-- 如果只有一张图 -->
        <view wx:if="{{item.imgUrl.length==1 || 1==1}}">
          <image class="main-image-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
        </view>

        <!-- 如果只有两张图 -->
        <view wx:elif="{{item.imgUrl.length==2}}">
          <image class="main-image-2-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          <image class="main-image-2-2" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
        </view>

        <!-- 如果只有三张图 -->
        <view wx:elif="{{item.imgUrl.length==3}}">

          <view class="pic-left-3">
            <image class="main-image-3-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
          <view class="pic-right-3">
            <image class="main-image-3-2" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-3-3" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
        </view>

        <!-- 如果只有四张图 -->
        <view wx:elif="{{item.imgUrl.length==4}}">
          <view class="pic-top-4">
            <image class="main-image-4-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-4-2" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
          <view>
            <image class="main-image-4-3" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-4-4" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
        </view>

        <!-- 如果只有五张图 -->
        <view wx:elif="{{item.imgUrl.length==5}}">
          <view class="pic-top-4">
            <image class="main-image-5-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-5-2" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
          <view>
            <image class="main-image-5-3" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-5-4" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-5-5" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
        </view>

        <!-- 如果只有六张图 -->
        <view wx:else>
          <view class="pic-left-6">
            <image class="main-image-3-1" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
          <view class="pic-right-6">
            <image class="main-image-3-2" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-3-3" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
          <view>
            <image class="main-image-5-3" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-5-4" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
            <image class="main-image-5-5" bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src="{{item.imgUrl}}"></image>
          </view>
        </view>
      </view>


      <view class="goods-and-like">

        <!-- 商品透出 -->
        <view class='like'>
          <!-- <image  catchtap='btn' data-id="{{item.id}}" src='https://c.static-nike.com/a/images/t_PDP_864_v1/f_auto,b_rgb:f5f5f5/e4i53xwtw6gyjufm6mym/joyride-run-fk-%E5%A5%B3%E5%AD%90%E8%B7%91%E6%AD%A5%E9%9E%8B-lL5WQ5.jpg'></image>
          <image  catchtap='btn' data-id="{{item.id}}" src='https://c.static-nike.com/a/images/t_PDP_864_v1/f_auto,b_rgb:f5f5f5/bkm2o7yf6drnjacbamqs/jordan-proto-max-720-%E7%94%B7%E5%AD%90%E8%BF%90%E5%8A%A8%E9%9E%8B-V7CRxd.jpg'></image> -->
          <image catchtap='add_like' video-or-image-id="{{item.id}}" src='data:image/png;base64,'></image>
        </view>

        <!-- 点赞，评论，分享图标 -->
        <!-- <view class='like'>
          
        </view> -->

      </view>



    </block>
  </view>
</view>