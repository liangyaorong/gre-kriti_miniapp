<view class="main">

  <!-- 轮播图 -->
  <view class='slides'>
    <image mode="aspectFill" src="https://gss3.bdstatic.com/7Po3dSag_xI4khGkpoWK1HF6hhy/baike/c0%3Dbaike92%2C5%2C5%2C92%2C30/sign=109e35ebacec08fa320d1bf538875608/95eef01f3a292df507633d41b0315c6035a873c6.jpg">
    </image>
  </view>

  <view class='head-title'>
    <text>5G未来城-超速美力</text>

  </view>


  <view class='nav_wrap'>

    <view class="navbar">
      <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
        <text>{{item}}</text>
      </lable>
    </view>
  </view>

  <!-- 首页导航 -->
  <view class='nav_wrap'>
    <!-- 自己滚动区域的组件 -->
    <scroll-view class='nav' scroll-x>
      <view bindtap='activeNav' data-index="{{index}}" class='nav_item {{index===currentIndexNav?"active":""}}' wx:for="{{navList}}" wx:key="{{index}}">
        {{item}}
      </view>
    </scroll-view>
  </view>
  <!--全部-->
  <view hidden="{{currentTab!='0'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{imagesList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.headerUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">{{item.name}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.timestamp}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.content}}
        </view>
        <!-- 图片容器 -->
        <view class='video_img'>
          <!-- 图片 -->
          <image bindtap='previewImg' data-scr="{{item.imgUrl}}" mode="aspectFill" src='{{item.imgUrl}}'></image>
        </view>

        <view class='other'>
          <view bindtap='order_details' data-index="1">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待付款</text>

          </view>
          <view bindtap='order_details' data-index="2">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待收货</text>
          </view>
          <view bindtap='order_details' data-index="3">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待评价</text>
          </view>
        </view>
      </view>
    </view>

  </view>

  <!-- 视频列表 -->
  <view hidden="{{currentTab!='1'}}">
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{videoList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.headerUrl}}"></image>
          <view class="text-box">
            <view class="arow arow01">
              <view class="artist-name">{{item.name}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.timestamp}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.content}}
        </view>
        <!-- 图片容器 -->
        <view class='video_img'>
          <!-- 图片 -->
          <video bindplay='watch' src='{{item.videoUrl}}'></video>
          <!-- <image mode="aspectFill" src='{{item.poster}}'></image> -->
        </view>

        <view class='other'>
          <view bindtap='order_details' data-index="1">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待付款</text>
          </view>
          <view bindtap='order_details' data-index="2">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待收货</text>
          </view>
          <view bindtap='order_details' data-index="3">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>待评价</text>
          </view>
        </view>



      </view>
    </view>

  </view>



</view>