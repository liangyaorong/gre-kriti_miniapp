<view>
  <SearchBar id="SearchBar" searchstr="{{searchstr}}" bind:searchList="searchList" bind:endsearchList="endsearchList" bind:cancelsearch="cancelsearch" bind:activity_clear="activity_clear">
  </SearchBar>
</view>


<!-- 头图 -->
<view class='slides'>
  <image mode="aspectFill" src="{{headerImageUrl}}"></image>
</view>

<!-- <view class='head-title'>
    <text>5G未来城-超速美力</text>
  </view> -->

<view class='nav_area'>
  <view class="navbar">
    <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
      <text>{{item}}</text>
    </lable>
  </view>

  <view class='nav_wrap'>
    <scroll-view class='nav' scroll-x>
      <view bindtap='activeNav' data-index="{{index}}" class='nav_item {{index===currentIndexNav?"active":""}}' wx:for="{{navList}}" wx:key="{{index}}">
        {{item}}
      </view>
    </scroll-view>
    <view class='nav_wrap_text'>
      <text bindtap="sortByTime">时间</text>
      <image bindtap="sortByTime" src="data:image/png;base64,"></image>
      <text bindtap="sortByLike">点赞</text>
      <image bindtap="sortByLike" src="data:image/png;base64,"></image>
    </view>

  </view>
</view>


<view class='total'>
  <!--图片-->
  <view hidden="{{currentTab!='0'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{imagesList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">{{item.wxName}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <!-- 图片容器 -->
        <view class='video_img'>
          <swiper indicator-dots="Boolean">
            <block wx:for="{{item.itemUrls}}" wx:for-item="url" wx:key="">
              <swiper-item>
                <image class='swiper_image' bindtap='previewImg' data-collectionid="{{item.collectionId}}" data-type='image' data-scr="{{url}}" data-scrlist="{{item.itemUrls}}" mode="aspectFill" src='{{url}}'></image>
              </swiper-item>
            </block>
          </swiper>
        </view>

        <view class='other'>
          <view>
            <image bindtap='like' data-collectionid="{{item.collectionId}}" data-type='image' hidden="{{item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <image hidden="{{!item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>


      </view>
    </view>
  </view>


  <!--视频-->
  <view hidden="{{currentTab!='1'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{videosList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">{{item.wxName}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <view class='video_img'>
          <video objectFit='cover' bindplay='watch' data-collectionid="{{item.collectionId}}" data-type='video' poster='http://n.sinaimg.cn/translate/w731h1024/20171130/Meqv-fypikws9430343.jpg' control='true' src='{{item.itemUrls[0]}}'></video>
        </view>

        <view class='other'>
          <view>
            <image bindtap='like' data-collectionid="{{item.collectionId}}"  data-type='video' hidden="{{item.isCurrentUserLiked}}"  src='data:image/png;base64,'></image>
            <image hidden="{{!item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>

      </view>
    </view>

  </view>
</view>