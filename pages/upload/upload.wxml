<!--导航条-->
<view class="navbar">
  <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="unique" bindtap="navbarTap">
    <text>{{item}}</text>
  </lable>
</view>



<!--图片-->
<view hidden="{{currentTab!='0'}}" class='total'>
  <text class="select-title">图片选择</text>

  <view class='share-pics' wx:if="{{upImagePathLen!=0}}">
    <view class='pics'>
      <view class='tp' wx:for="{{upImgArr}}" wx:key="item">
        <image mode='aspectFill' class='img' src='{{item.path}}' data-presrc="{{item.path}}" bindtap='previewImg' />
        <image class='delbtn' data-index="{{index}}" data-type="image" bindtap='delFile' src='data:image/png;base64,'></image>
      </view>

    </view>
  </view>

  <view class='add-btn' wx:if="{{upImageFilesBtn}}" data-type="image" bindtap='chooseFiles'></view>




  <form bindsubmit="bindFormSubmitPics">
    <textarea class='desc' maxlength='50' style="height: 5em" placeholder="描述即将发表的作品" name="textarea" />
    <text class="tag-title">标签</text>

    <view class='nav_wrap'>
      <!-- 自己滚动区域的组件 -->
      <scroll-view class='nav' scroll-x>
        <view bindtap='activeNav' data-index="{{index}}" class='nav_item {{index===currentIndexNav?"active":""}}' wx:for="{{navList}}" wx:key="{{index}}">
          {{item}}
        </view>
      </scroll-view>
    </view>
    <button form-type="submit" type='primary'> 发表 </button>
  </form>


</view>

<!--视频-->
<view hidden="{{currentTab!='1'}}" class='total'>
  <!-- 选文件 -->
  <text class="select-title">视频选择</text>

  <view class='share-vids' wx:if="{{upVideoPathLen!=0}}">
    <view class='videos'>
      <block wx:for="{{upVideoArr}}" wx:for-item="v" wx:key="k">
        <view class='vid'>
          <video class='video' src="{{v.tempFilePath}}" poster='{{v.thumbTempFilePath}}'></video>
        </view>
        <image class='delbtn' data-index="{{index}}" data-type="video" bindtap='delFile' src='data:image/png;base64,'></image>
      </block>
    </view>
  </view>

  <view class='add-btn' hidden="{{!upVideoFilesBtn}}" data-type="video" bindtap='chooseFiles'>
  </view>

  <form bindsubmit="bindFormSubmitVideos">
    <textarea class='desc' maxlength='50' style="height: 5em" placeholder="描述即将发表的作品" name="textarea" />
    <text class="tag-title">标签</text>

    <view class='nav_wrap'>
      <!-- 自己滚动区域的组件 -->
      <scroll-view class='nav' scroll-x>
        <view bindtap='activeNav' data-index="{{index}}" class='nav_item {{index===currentIndexNav?"active":""}}' wx:for="{{navList}}" wx:key="{{index}}">
          {{item}}
        </view>
      </scroll-view>
    </view>
    <button form-type="submit" type='primary'> 发表 </button>
  </form>

</view>