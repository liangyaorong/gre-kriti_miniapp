<view class="navbar" hidden="{{showVideoOnly}}">
  <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
    <text>{{item}}</text>
  </lable>
</view>
<view class="main">



  <!--图片-->
  <view hidden="{{currentTab!='0'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{imagesList}}" wx:for-index="idx" wx:key="item">
        <view class="artist-name">{{item.wxName}}</view>

        <view class='video_title'>{{item.post}}</view>

        <!-- 图片容器 -->
        <view class='video_img'>
          <swiper indicator-dots="Boolean">
            <block wx:for="{{item.itemUrls}}" wx:for-item="url" wx:key="">
              <swiper-item>
                <image class='swiper_image' bindtap='previewImg' data-scr="{{url}}" data-scrlist="{{item.itemUrls}}" data-idx="{{idx}}" mode="aspectFill" src='{{url}}'></image>
              </swiper-item>
            </block>
          </swiper>
        </view>

        <view class='other'>
          <view bindtap='pass' data-collectionid="{{item.collectionId}}" data-idx="{{idx}}" data-type="image">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>通过</text>

          </view>
          <view bindtap='notPass' data-collectionid="{{item.collectionId}}" data-idx="{{idx}}" data-type="image">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>不通过</text>
          </view>
        </view>


      </view>
    </view>

  </view>


  <!--视频-->
  <view hidden="{{currentTab!='1'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{videosList}}" wx:for-index="idx" wx:key="{{index}}">

        <view class='video_title'>{{item.post}}
        </view>

        <!-- 图片容器 -->
        <view class='video_img'>
          <!-- 图片 -->
          <video objectFit='cover' src='{{item.itemUrls}}'></video>
          <!-- <image mode="aspectFill" src='{{item.poster}}'></image> -->
        </view>


        <view class='other'>
          <view bindtap='pass' data-collectionid="{{item.collectionId}}" data-idx="{{idx}}" data-type="video">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>通过</text>

          </view>
          <view bindtap='notPass' data-collectionid="{{item.collectionId}}" data-idx="{{idx}}" data-type="video">
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>不通过</text>
          </view>
        </view>


      </view>
    </view>

  </view>


</view>