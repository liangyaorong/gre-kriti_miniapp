<view>
  <view class='box'>
    <view class='my-name'>
      <image src="{{avatarUrl}}"></image>
      <text class="Username">{{nickName}}</text>
    </view>

    <view hidden="{{!isAdmin}}">
      <button class='shenhe' bindtap="goCheck">你是管理员，前往审核</button>
    </view>

    <view hidden="{{phone != ''}}">
      <button class='shenhe' open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber"> 授权手机号,参与活动吧</button>
    </view>

    <view class="navbar" hidden="{{showVideoOnly}}">
      <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
        <text>{{item}}</text>
      </lable>
    </view>
  </view>






  <!--图片-->
  <view hidden="{{currentTab!='0'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{imagesList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">ID: {{item.collectionId}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>

        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <!-- 图片容器 -->
        <view class='video_img'>

          <swiper indicator-dots="Boolean">
            <block wx:for="{{item.itemUrls}}" wx:for-item="url" wx:key="">
              <swiper-item>
                <image class='swiper_image' bindtap='previewImg' data-scr="{{url}}" data-scrlist="{{item.itemUrls}}" mode="aspectFill" src='{{url}}'></image>
              </swiper-item>
            </block>
          </swiper>
        </view>

        <view class='other'>
          <view>
            <image data-collectionid="{{item.collectionId}}" data-type='image' data-idx='{{idx}}' hidden="{{item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <image hidden="{{!item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>
      </view>
    </view>
  </view>


  <!--视频-->
  <view hidden="{{currentTab!='1'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{videosList}}" wx:for-index="idx" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">ID: {{item.collectionId}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>
          <view class="text-box2">
              <view class="notPass" hidden="{{item.pass=='passed'}}">审核未通过</view>
              <view class="pass" hidden="{{item.pass!='passed'}}">审核通过</view>
          </view>
        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <view class='video_img'>
          <video id='{{item.collectionId}}' objectFit='cover' bindplay='watch' data-collectionid="{{item.collectionId}}" data-type='video' data-idx='{{idx}}' data-watched='{{item.isCurrentUserPlayed}}' src='{{item.itemUrls[0]}}' preload controls></video>
        </view>

        <view class='other'>
          <view>
            <image data-collectionid="{{item.collectionId}}" data-type='image' data-idx='{{idx}}' hidden="{{item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <image hidden="{{!item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>



      </view>
    </view>

  </view>


</view>