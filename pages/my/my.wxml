<view>

  <view hidden="{{nickName!='' && avatarUrl!=''}}">
    <view class='header'>
      <image src='https://gss3.bdstatic.com/7Po3dSag_xI4khGkpoWK1HF6hhy/baike/c0%3Dbaike92%2C5%2C5%2C92%2C30/sign=109e35ebacec08fa320d1bf538875608/95eef01f3a292df507633d41b0315c6035a873c6.jpg'></image>
    </view>


    <view class='content'>
      <view>申请获取以下权限</view>
      <text>获得你的公开信息(昵称，头像等)</text>
    </view>
    <button open-type="getUserInfo" type='primary' bindgetuserinfo="onGotUserInfo">授权登陆</button>
  </view>


  <view hidden="{{nickName==''||avatarUrl==''}}">
    <view class='box'>
      <view class='my-name'>
        <image src="{{avatarUrl}}"></image>
        <text class="Username">{{nickName}}</text>
      </view>
    </view>

    <view class='nav_wrap'>
      <view class="navbar">
        <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
          <text>{{item}}</text>
        </lable>
      </view>
    </view>


    <!--图片-->
    <view hidden="{{currentTab!='0'}}">
      <!-- 图片列表 -->
      <view class='video_wrap'>
        <view class='video_item' wx:for="{{imagesList}}" wx:key="{{index}}">

          <view class='artist'>
            <image src="{{item.wxHeadUrl}}"></image>
            <view class="text-box">
              <view class="arow01">
                <view class="artist-name">{{item.wxName}}</view>
              </view>
              <view class="arow">
                <view class="time">{{item.createTime}}</view>
              </view>
            </view>
          </view>

          <view class='video_title'>{{item.post}}
          </view>

          <!-- 图片容器 -->
          <view class='video_img'>

            <swiper indicator-dots="Boolean">
              <block wx:for="{{item.itemUrls}}" wx:for-item="url" wx:key="">
                <swiper-item>
                  <image class='swiper_image' bindtap='previewImg' data-scr="{{url}}" data-scrlist="{{item.itemUrls}}" mode="aspectFill" src='{{url}}'></image>
                </swiper-item>
              </block>
            </swiper>
          </view>

          <view class='other'>
            <view bindtap='order_details' data-index="1">
              <image src='data:image/png;base64,'></image>
              <text class='icontext'>{{item.likeCount}}</text>

            </view>
            <view bindtap='order_details' data-index="2">
              <image src='data:image/png;base64,'></image>
              <text class='icontext'>{{item.likeCount}}</text>
            </view>
          </view>


        </view>
      </view>

    </view>


    <!--视频-->
    <view hidden="{{currentTab!='1'}}">
      <!-- 图片列表 -->
      <view class='video_wrap'>
        <view class='video_item' wx:for="{{videosList}}" wx:key="{{index}}">

          <view class='artist'>
            <image src="{{item.wxHeadUrl}}"></image>
            <view class="text-box">
              <view class="arow01">
                <view class="artist-name">{{item.wxName}}</view>
              </view>
              <view class="arow">
                <view class="time">{{item.createTime}}</view>
              </view>
            </view>
          </view>

          <view class='video_title'>{{item.post}}
          </view>

          <!-- 图片容器 -->
          <view class='video_img'>
            <!-- 图片 -->
            <video bindplay='watch' src='{{item.itemUrls}}' poster="{{item.poster}}" controls='true'></video>
            <!-- <image mode="aspectFill" src='{{item.poster}}'></image> -->
          </view>

          <view class='other'>
            <view bindtap='order_details' data-index="1">
              <image src='data:image/png;base64,'></image>
              <text class='icontext'>{{item.likeCount}}</text>

            </view>
            <view bindtap='order_details' data-index="2">
              <image src='data:image/png;base64,'></image>
              <text class='icontext'>{{item.likeCount}}</text>
            </view>
          </view>


        </view>
      </view>

    </view>


  </view>
</view>