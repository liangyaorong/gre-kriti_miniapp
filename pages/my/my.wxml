<view>
  <view class='box'>
    <view class='my-name'>
      <image src="{{avatarUrl}}"></image>
      <text class="Username">{{nickName}}</text>
    </view>

    <view hidden="{{!isAdmin}}" class= 'shenhe'>
      <button bindtap="goCheck" type='primary'>你是管理员，前往审核</button>
    </view>

    <view hidden="{{phone != ''}}"  class= 'shenhe'>
      <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" type='primary'> 授权手机号,参与活动吧</button>
    </view>
  </view>

  <view class="navbar">
    <lable wx:for="{{navbar}}" data-idx="{{index}}" class="item {{currentTab==index ? 'active' : ''}}" wx:key="" bindtap="navbarTap">
      <text>{{item}}</text>
    </lable>
  </view>




  <!--图片-->
  <view hidden="{{currentTab!='0'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{imagesList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">ID: {{item.collectionId}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <!-- 图片容器 -->
        <view class='video_img'>

          <swiper indicator-dots="Boolean">
            <block wx:for="{{item.itemUrls}}" wx:for-item="url" wx:key="">
              <swiper-item>
                <image class='swiper_image' bindtap='previewImg' data-scr="{{url}}" data-scrlist="{{item.itemUrls}}" mode="aspectFill" src='{{url}}'></image>
              </swiper-item>
            </block>
          </swiper>
        </view>

      <view class='other'>
          <view>
            <image src='data:image/png;base64,'></image>
            
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>


      </view>
    </view>

  </view>


  <!--视频-->
  <view hidden="{{currentTab!='1'}}">
    <!-- 图片列表 -->
    <view class='video_wrap'>
      <view class='video_item' wx:for="{{videosList}}" wx:key="{{index}}">

        <view class='artist'>
          <image src="{{item.wxHeadUrl}}"></image>
          <view class="text-box">
            <view class="arow01">
              <view class="artist-name">{{item.wxName}}</view>
            </view>
            <view class="arow">
              <view class="time">{{item.createTime}}</view>
            </view>
          </view>
        </view>

        <view class='video_title'>{{item.post}}
        </view>

        <!-- 图片容器 -->
        <view class='video_img'>
          <!-- 图片 -->
          <video objectFit='cover' bindplay='watch' src='{{item.itemUrls}}' poster="{{item.poster}}" controls='true'></video>
          <!-- <image mode="aspectFill" src='{{item.poster}}'></image> -->
        </view>

      <view class='other'>
          <view>
            <image bindtap='like' data-collectionid="{{item.collectionId}}" hidden="{{item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <image hidden="{{!item.isCurrentUserLiked}}" src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.likeCount}}</text>

          </view>
          <view>
            <image src='data:image/png;base64,'></image>
            <text class='icontext'>{{item.playCount}}</text>
          </view>
        </view>


      </view>
    </view>

  </view>


</view>